{
  "name": "electron-express",
  "productName": "T-Runner",
  "version": "1.0.0",
  "description": "Example of embedding a node express server within an electron app",
  "main": "main.js",
  "engine": {
    "node": ">=10.15.1"
  },
  "scripts": {
    "start": "electron .",
    "build-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=oss.icns --prune=true --out=builds",
    "build-win": "electron-packager . --overwrite --platform=win32 --arch=x64 --prune=true --out=builds",
    "build-ng": "cd ./http && npm run prod-ng && cd ../",
    "win-one": "npm build-ng && npm run build-win",
    "win-empty-electron": "rm -rf ./builds/T-Runner-win32-x64/resources/app/http/{public,ng}",
    "win-fill-electron": "cp -r ./http/public ./builds/T-Runner-win32-x64/resources/app/http/",
    "win-shim-electron":"npm run build-ng && npm run win-empty-electron && npm run win-fill-electron"
  },
  "repository": "https://github.com/jason/electron-express",
  "keywords": [
    "node.js",
    "express",
    "electron"
  ],
  "author": "Jason Henderson",
  "devDependencies": {
    "electron": "^4.0.5",
    "electron-packager": "^13.0.1"
  },
  "dependencies": {
    "electron-log": "^3.0.1",
    "linebyline": "^1.3.0"
  }
}
